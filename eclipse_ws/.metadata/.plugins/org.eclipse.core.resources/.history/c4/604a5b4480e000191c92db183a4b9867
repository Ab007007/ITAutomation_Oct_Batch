package com.itautomation.training.variable;

public class GlobalVariablesDemo 
{
	
	static int x=100;

	//method
	public static void main(String[] args)
	{
		int i;
		i = 10;
		System.out.println(i);
		System.out.println(x);
		test();
		
		
	}

	
	public static void test()
	{
		int i;
		i = 20;
		System.out.println(i);
		System.out.println(x);
	}
}
